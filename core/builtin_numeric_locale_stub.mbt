fn format_datetime_with_c_strftime(
  pattern : String,
  year : Int,
  month : Int,
  day : Int,
  hour : Int,
  minute : Int,
  second : Double,
  use_localtime : Bool,
) -> String? {
  ignore(pattern)
  ignore(year)
  ignore(month)
  ignore(day)
  ignore(hour)
  ignore(minute)
  ignore(second)
  ignore(use_localtime)
  None
}
